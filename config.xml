<?xml version='1.0' encoding='utf-8'?>
<widget id="com.playinitium" version="1.0.20" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Initium</name>
    <description>
        Initium MMORPG.
    </description>
    <author email="narmstrong@playinitium.com" href="https://www.playinitium.com">
        Universe Projects Inc
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    
    <preference name="android-targetSdkVersion" value="30" />        
</widget>
